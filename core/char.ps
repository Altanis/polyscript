type char = __BUILTIN_CHAR__;

impl TClonable for char {
    fn clone(&self) -> Self {
        return *self;
    }
}

impl TCopyable for char {}

impl TEquatable for char {
    fn equate(&self, other: &Self) -> bool {
        return *self == *other;
    }
}

impl TDefault for char {
    fn default() -> Self {
        return '\0';
    }
}