struct Iterable {}

trait Iterator {
    type Item;

    fn next(&this): Self.Item;
}

impl Iterator for Iterable {
    type Item = int;

    fn next(&this): Self.Item {
        return 3;
    }
}

fn next<I: Iterator>(x: &I): [I as Iterator].Item {
    return x.next();
}

let iterator = Iterable {};
let x: int = next(&iterator);