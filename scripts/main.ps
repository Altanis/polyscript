struct Scalar {
    private x: int;
}

impl Scalar {
    fn new(x: int): Scalar {
        Scalar { x }
    }

    fn get_x(&self): int {
        self.x
    }
}

let scalar = Scalar.new(4);
let x = scalar.get_x();

print(scalar);
print(x);

