type Integer = int;

fn add[T: Add](a: T, b: T): T {
    return a + b;
}

struct Point[T] {
    public x: T;
    public y: T;
}

trait VectorLike[T] {
    fn new(): Point;
    fn operation[T](a: Point[T], b: Point[T]): Integer;
}

impl VectorLike[Integer] for Point[Integer] {
    fn add(a: Point[Integer], b: Point[Integer]): Integer {
        return add(a.x, a.y);
    }
}