struct HeavyData<T> {
    x: int;
}

impl<T: Clone> Clone for HeavyData<T> {
    fn clone(&self): Self {
        Self { x: [T as Clone].clone(&self.x) }
    }
}

let x = HeavyData { x: 4 };
let y = [HeavyData<int> as Clone].clone(&x);