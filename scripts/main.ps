trait Animal {
    type SpeechEmission;

    fn speak(&this): Self.SpeechEmission;
}

struct Dog {}

impl Animal for Dog {
    type SpeechEmission = char;
    fn speak(&this): Self.SpeechEmission {
        'b'
    }
}

impl Dog {
    type SpeechEmission = float;
}

let dog = Dog {};
let f: [Dog as Animal].SpeechEmission = dog.speak();