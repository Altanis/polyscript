struct Point[T] {
    public x: T;
    public y: T;
}

impl Add for Point[int] {
    type Output = Point[int];

    fn add(this, other: Self): Self.Output {
        return Point {
            x: self.x + other.x,
            y: self.y + other.y
        };
    }
}

impl Add for Point[float] {
    type Output = Point[float];

    fn add(this, other: Self): Self.Output {
        return Point {
            x: self.x + other.x,
            y: self.y + other.y
        };
    }
}

impl Add[Point[int]] for Point[float] {
    type Output = Point[float];

    fn add(this, other: Point[int]): Self.Output {
        return Point {
            x: self.x + other.x,
            y: self.y + other.y
        };
    }
}

