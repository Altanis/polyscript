struct Point {
    public x: int;
    public y: int;
}

trait Add<Rhs> {
    type Output;
    fn add(&this, other: Rhs): Self.Output;
}

impl Add<Point> for Point {
    type Output = Point;

    fn add(&this, other: Point): Point {
        let new_x = this.x + other.x;
        let new_y = this.y + other.y;
        return Point { x: new_x, y: new_y };
    }
}