struct Vector2D {
    x: int;
    y: int;
}

enum Difficulty {
    Easy,
    Medium = 3,
    Hard
}

impl Vector2D {
    const DIMENSIONS: int = 2;
    const DIFFICULTY: Difficulty = Difficulty.Medium;

    fn new(x: int, y: int): Self {
        Vector2D {
            x,
            y
        }
    }
}

