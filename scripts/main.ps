let x: int = 5 + 3;
let y: float = 5.0 + 3.0;
let z: float = 5.0 / 3.0;
let a: int = 5 / 3;

struct Vector2D {
    x: int;
    y: int;
}

impl Add<Vector2D> for Vector2D {
    type Output = Vector2D;
    fn add(&this, other: &Self): Self.Output {
        Vector2D {
            x: this.x + other.x,
            y: this.y + other.y
        }
    }
}

impl Multiply<int> for Vector2D {
    type Output = Vector2D;
    fn add(&this, other: int): Self.Output {
        Vector2D {
            x: this.x * other,
            y: this.y * other
        }
    }
}

let v1 = Vector2D { x: 0, y: 0 };
let v2 = Vector2D { x: 0, y: 0 };
let v3 = v1 + v2;
let v4 = v1 * 2;